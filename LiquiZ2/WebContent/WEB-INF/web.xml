<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<display-name></display-name>

	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>Util</servlet-name>
		<servlet-class>org.adastraeducation.liquiz.util.Util</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Util</servlet-name>
		<url-pattern>/addQuestion</url-pattern>
	</servlet-mapping>

	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>LoginJudge</servlet-name>
		<servlet-class>org.adastraeducation.liquiz.servelet.LoginJudge</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginJudge</servlet-name>
		<url-pattern>/LoginJudge</url-pattern>
	</servlet-mapping>

	<servlet>
		<description>This is the description of my J2EE component</description>
		<display-name>This is the display name of my J2EE component</display-name>
		<servlet-name>Register</servlet-name>
		<servlet-class>org.adastraeducation.liquiz.servelet.Register</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Register</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>


	<welcome-file-list>
		<welcome-file>Login.jsp</welcome-file>
	</welcome-file-list>
</web-app>
